{
    "name": "okorneliuk/symfony-websms-notifier",
    "type": "symfony-bridge",
    "description": "Symfony WebSms Notifier Bridge",
    "keywords": ["sms", "websms", "notifier"],
    "homepage": "https://github.com/okorneliuk",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleh Korneliuk",
            "email": "oleh.korneliuk@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-iconv": "*",
        "symfony/http-client": "^6.4|^7.2",
        "symfony/notifier": "^6.4|^7.2",
        "symfony/polyfill-php82": "^1.31"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^7.2"
    },
    "autoload": {
        "psr-4": { "Okorneliuk\\Symfony\\NotifierBridge\\WebSms\\": "" },
